<h1>Keyboards</h1>

<%= link_to 'Create new keyboard', [:new, @user, :keyboard] %>


<ul>
	<% @keyboards.each do |keyboard| %>
		<li>
			<%= link_to keyboard.name, [@user, keyboard] %>

			<%= link_to 'Edit', [:edit, @user, keyboard] %>

			<%= button_to 'Delete', [@user, keyboard], method: :delete %>

			<%= link_to 'Commits', [@user, keyboard, :commits] %>

			<%= link_to 'HEAD', [@user, keyboard, keyboard.commits.last] %>
		</li>
	<% end %>
</ul>