<%= form_for([@user, @keyboard, @commit], url: user_keyboard_commits_path(@user.name, @keyboard.name)) do |f| %>

  <%= f.text_field :name %>

  <%= f.text_area :description %>



  <%= f.hidden_field :parent_id, :value => @commit.id %>

  <%= f.hidden_field :layers %>


  <%= f.submit name: "submit" %>

<% end %>