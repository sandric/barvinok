<h1><%= @keyboard.name %></h1>

<h2><%= @commit.name %></h2>

<%= link_to 'Fork', fork_keyboard_commit_path(@keyboard, @commit), :method => :post %>

<div id="app">
</div>

<div id="text">
</div>


<%= javascript_tag do %>

window.editor = CodeMirror(document.getElementById("text"), {
        mode:  "javascript",
        lineNumbers: true
      });

yizhackclj.components.initialize(<%= raw @commit.data.to_json %>);

<% end %>